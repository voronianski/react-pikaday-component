{
  "name": "react-pikaday-component",
  "version": "2.1.5-field-type",
  "description": "React component wrapper around Pikaday.js",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build": "npm run build-babel && npm run build-dist-dev && npm run build-dist-prod",
    "build-dist-dev": "NODE_ENV=development webpack --config webpack.config --colors",
    "build-dist-prod": "NODE_ENV=production webpack --config webpack.config --progress --colors -p",
    "build-babel": "babel ./src/ReactPikadayComponent.js --out-file index.js",
    "build-example": "NODE_ENV=production browserify ./example/index.js -t [ babelify --presets [ es2015 stage-0 react ] --plugins [ transform-object-assign ] ] | uglifyjs -cm > ./example/bundle.min.js",
    "lint": "eslint ./src",
    "lint-dev": "eslint --fix ./src",
    "test": "echo \"Comming soon ;)\" && exit 0",
    "prepublish": "npm run build-babel && npm run build-dist-prod"
  },
  "keywords": [
    "react",
    "component",
    "datepicker",
    "isomorphic",
    "universal",
    "pikaday"
  ],
  "author": "Dmitri Voronianski <dmitri.voronianski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/voronianski/react-pikaday-component/issues"
  },
  "homepage": "https://github.com/voronianski/react-pikaday-component#readme",
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "basscss": "^7.1.1",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.3",
    "pikaday": "^1.4.0",
    "prop-types": "^15.5.10",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "sinon": "^1.17.3",
    "uglify-js": "^2.6.2",
    "webpack": "^1.12.14",
    "webpack-umd-external": "^1.0.2"
  },
  "dependencies": {}
}
